{
  "version": 2,
  "name": "ip-web-mobile",
  "builds": [
    {
      "src": "mobile_web.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mobile_web.py"
    }
  ],
  "env": {
    "FLET_WEB_USE_COLOR_EMOJI": "true",
    "PYTHONPATH": "/var/task"
  }
}